<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/anjulata-restapi/post">
        <service class="Anjulata\RestApi\Api\PostManagementInterface" method="getPost"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V2/product/getProductById/:id">
        <service class="Anjulata\RestApi\Api\ProductRepositoryInterface" method="getProductById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>
